{
    "log": {
        "level": "info",
        "output": "sing-box.log",
        "timestamp": true
    },
    "experimental": {
        "cache_file": {
            "enabled": true,
            "store_fakeip": true
        },
        "clash_api": {
            "default_mode": "Rule",
            "external_controller": "127.0.0.1:9090",
            "external_ui": "ui",
            "external_ui_download_url": "https://github.com/Zephyruso/zashboard/releases/latest/download/dist.zip",
            "external_ui_download_detour": "GitHub"
        }
    },
    "dns": {
        "servers": [
            {
                "tag": "google",
                "type": "https",
                "server": "dns.google",
                "detour": "GLOBAL",
                "domain_resolver": "hosts"
            },
            {
                "tag": "ali",
                "type": "https",
                "server": "dns.alidns.com",
                "domain_resolver": "hosts"
            },
            {
                "tag": "fakeip",
                "type": "fakeip",
                "inet4_range": "198.18.0.0/15",
                "inet6_range": "fc00::/18"
            },
            {
                "type": "hosts",
                "tag": "hosts",
                "predefined": {
                    "dns.alidns.com": [
                        "223.5.5.5",
                        "223.6.6.6"
                    ],
                    "dns.google": [
                        "8.8.8.8",
                        "8.8.4.4"
                    ]
                }
            }
        ],
        "rules": [
            {
                "clash_mode": "Direct",
                "server": "ali"
            },
            {
                "clash_mode": "Global",
                "server": "fakeip"
            },
            {
                "action": "predefined",
                "query_type": [
                    "HTTPS"
                ],
                "rcode": "NOTIMP"
            },
            {
                "query_type": [
                    "A",
                    "AAAA"
                ],
                "server": "fakeip",
                "rewrite_ttl": 1
            },
            {
                "rule_set": [
                    "Apple",
                    "ChinaDomain",
                    "Steam@CN"
                ],
                "server": "ali"
            }
        ],
        "final": "google",
        "independent_cache": true,
        "strategy": "prefer_ipv4"
    },
