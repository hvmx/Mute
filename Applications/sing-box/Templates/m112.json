    "experimental": {
        "cache_file": {
            "enabled": true,
            "store_fakeip": true
        }
    },
    "dns": {
        "servers": [
            {
                "tag": "global",
                "type": "https",
                "server": "1.1.1.1",
                "detour": "GLOBAL"
            },
            {
                "tag": "direct",
                "type": "https",
                "server": "223.6.6.6"
            },
            {
                "tag": "fakeip",
                "type": "fakeip",
                "inet4_range": "198.18.0.0/15"
            }
        ],
        "rules": [
            {
                "clash_mode": "Direct",
                "server": "direct"
            },
            {
                "clash_mode": "Global",
                "server": "fakeip"
            },
            {
                "query_type": "HTTPS",
                "action": "reject"
            },
            {
                "query_type": [
                    "A",
                    "AAAA"
                ],
                "server": "fakeip",
                "rewrite_ttl": 1
            },
            {
                "rule_set": [
                    "Apple",
                    "ChinaDomain",
                    "Steam@CN"
                ],
                "server": "direct"
            }
        ],
        "final": "global",
        "independent_cache": true,
        "strategy": "ipv4_only"
